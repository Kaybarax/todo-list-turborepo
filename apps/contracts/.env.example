# Blockchain Contracts Environment Variables
# Copy this file to .env.local for local development

# Blockchain RPC URL
MOONBEAM_RPC_URL=http://localhost:9933

# Private key for contract deployment (keep this secure!)
PRIVATE_KEY=0xYourPrivateKeyHere

# Gas reporting for contract deployment
REPORT_GAS=true

# Block explorer API key
MOONSCAN_API_KEY=YourMoonscanApiKeyHere

# Network configuration
NETWORK_ID=1287
GAS_PRICE=1000000000
