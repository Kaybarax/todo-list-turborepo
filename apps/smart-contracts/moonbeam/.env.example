# Moonbeam Network Configuration

# Private key for deployment (without 0x prefix)
MOONBEAM_PRIVATE_KEY=your_private_key_here
# Alternative: use the general PRIVATE_KEY if you prefer
PRIVATE_KEY=your_private_key_here

# Moonbeam Mainnet RPC URL
MOONBEAM_RPC_URL=https://rpc.api.moonbeam.network

# Moonbase Alpha Testnet RPC URL
MOONBASE_RPC_URL=https://rpc.api.moonbase.moonbeam.network

# Moonscan API Key for contract verification
MOONSCAN_API_KEY=your_moonscan_api_key_here

# Gas reporting configuration
REPORT_GAS=true
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Local development node (if running Moonbeam locally)
MOONBEAM_DEV_RPC_URL=http://localhost:9933