{
  "name": "@todo/ui-mobile",
  "version": "0.1.0",
  "description": "React Native component library for the Todo app",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "npm run build:tokens && vite build",
    "build:lib": "npm run build:tokens && vite build",
    "build:tokens": "style-dictionary build",
    "analyze:bundle": "node scripts/bundle-analysis.js",
    "build:tokens:watch": "style-dictionary build --watch",
    "dev": "npm run build:tokens && vite build --watch",
    "typecheck": "tsc --project tsconfig.dev.json --noEmit",
    "lint": "eslint \"lib/**/*.{ts,tsx}\" --ignore-pattern \"**/*.d.ts\" --ignore-pattern \"**/*.map\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-id=Project:68a509e2239206f05e7bab14",
    "chromatic:ci": "npx chromatic --project-id=Project:68a509e2239206f05e7bab14 --exit-zero-on-changes --exit-once-uploaded",
    "visual-test": "npm run build-storybook && npm run chromatic",
    "visual-test:ci": "npm run build-storybook && npm run chromatic:ci",
    "test:a11y": "playwright test --config=.storybook/playwright-a11y.config.ts",
    "test:visual": "playwright test --config=.storybook/playwright-visual.config.ts",
    "test:performance": "playwright test --config=.storybook/playwright-performance.config.ts",
    "test:storybook": "test-storybook",
    "test:storybook:ci": "test-storybook --ci",
    "showcase:start": "cd showcase && npm start",
    "showcase:build": "cd showcase && npm run build",
    "showcase:android": "cd showcase && npm run android",
    "showcase:ios": "cd showcase && npm run ios",
    "showcase:web": "cd showcase && npm run web",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@todo/services": "workspace:*",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "^5.3.1",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react-dom": "19.0.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@chromatic-com/storybook": "^2.0.2",
    "@playwright/test": "^1.40.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.16.0",
    "@testing-library/react-native": "^13.3.0",
    "@todo/config-eslint": "workspace:*",
    "@todo/config-ts": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.1",
    "@vitejs/plugin-react": "^5.0.0",
    "axe-core": "^4.8.0",
    "axe-playwright": "^2.0.0",
    "babel-loader": "^10.0.0",
    "chromatic": "^13.1.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-storybook": "^9.1.2",
    "jest": "^30.0.5",
    "jsdom": "^26.1.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-test-renderer": "19.1.1",
    "storybook": "^8.6.14",
    "style-dictionary": "^5.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-native": ">=0.70.0"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
